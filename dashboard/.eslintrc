{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "valid-jsdoc": 2,
    "no-undef": 2,
    "id-length": 0,
    "no-multi-spaces": 0,
    "func-names": 0,
    "comma-dangle": [2, "never"],
    "brace-style": [2,
      "stroustrup", {
      "allowSingleLine": false
    }],
    "no-multiple-empty-lines": [1, {max: 1}],
    "no-use-before-define": [2, "nofunc"]
  }
}
